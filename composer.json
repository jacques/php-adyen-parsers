{
    "name": "jacques/php-adyen-parsers",
    "description": "Adyen Parsers for PHP",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "league/csv": "^8.2",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0.0"
    },
    "require-dev": {
        "phpro/grumphp": "^0.15.2",
        "phpunit/php-invoker": "^2.0",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Adyen\\Parsers\\": "src/"
        }
    },
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@powertrip.co.za"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
